{
  "name": "rubber-band",
  "version": "0.1.1",
  "description": "Unapologetically evergreen-only supporting iframe resizer",
  "main": "library/",
  "scripts": {
    "start": "npm run watch",
    "build": "npm run babel && npm run browserify",
    "clean": "rm -rf ./library/** && rm -rf ./distribution/**",
    "watch": "babel --watch source --out-dir library",
    "babel": "babel source --out-dir library",
    "browserify": "browserify library/rubberband.content.js -o distribution/rubberband.content.js && browserify library/rubberband.host.js -o distribution/rubberband.host.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.sinnerschrader.com:frontend/rubber-band.git"
  },
  "keywords": [
    "iframe",
    "resizer"
  ],
  "author": {
    "name": "Team UX",
    "email": "team-ux@sinnerschrader.com"
  },
  "contributors": [
    {
      "name": "Mario Nebl",
      "email": "mario.nebl@sinnerschrader.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "iojs": ">= 1.4.3",
    "node": ">= 0.12.0",
    "npm": ">= 2.5.1"
  },
  "devDependencies": {
    "babel": "^5.2.14",
    "babel-eslint": "^3.0.1",
    "babelify": "^6.0.2",
    "browserify": "^10.1.0",
    "eslint": "^0.20.0"
  },
  "dependencies": {
    "lodash.throttle": "^3.0.3",
    "object-assign": "^2.0.0"
  }
}
